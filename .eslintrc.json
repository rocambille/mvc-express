{
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "parser": "babel-eslint",
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "radix": "off",
    "no-console": "off",
    "indent": ["error", 2],
    "linebreak-style": ["error", "unix"]
  }
}
